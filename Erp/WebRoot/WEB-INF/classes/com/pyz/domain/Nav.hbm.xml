<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.pyz.domain">
<class name="Nav" table="nav">
<id name="id" column="id"> </id>
<property name="state">
<column name="state" not-null="true" length="64"></column>
</property>
<property name="text">
<column name="text" not-null="true" length="64"></column>
</property>
<property name="url">
<column name="url" not-null="false" length="128"></column>
</property>
<property name="tid">
<column name="tid" not-null="true" length="10"></column>
</property>
</class>

</hibernate-mapping>